<p id="notice"><%= notice %></p>

<h1>Taged News</h1>

<table>
  <thead>
    <tr>
      <th>Tag</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @taged_news.each do |taged_news| %>
      <tr>
        <td><%= taged_news.tag %></td>
        <td><%= taged_news.status %></td>
        <td><%= link_to 'Show', taged_news %></td>
        <td><%= link_to 'Edit', edit_taged_news_path(taged_news) %></td>
        <td><%= link_to 'Destroy', taged_news, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Taged News', new_taged_news_path %>
